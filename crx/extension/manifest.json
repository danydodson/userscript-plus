{
  "manifest_version": 2,
  "name": "Userscript Plus",
  "description": "Show all UserJS for current site.",
  "author": "dany <danydodosn@gmail.com>",
  "version": "2.4.0",
  "homepage_url": "https://github.com/danydodson/userscript-plus",
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "96": "icons/icon_96.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "96": "icons/icon_96.png",
      "128": "icons/icon_128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  }
}
