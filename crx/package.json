{
  "name": "userscript-plus",
  "version": "2.4.0",
  "description": "Show all UserJS for current site. The easier way to install UserJs for Tampermonkey.",
  "homepage": "https://github.com/danydodson/userscript-plus#readme",
  "repository": "github:danydodson/userscript-plus",
  "author": "danydodson",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "zip": "rm -f extension.zip && zip -r extension.zip extension/",
    "crx": "rm -f extension.crx && crx pack ./extension -p extension.pem -o extension.crx"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "fuzzy.js": "^0.1.0",
    "iview": "2.12.0",
    "timeago.js": "^3.0.2",
    "vue": "^2.4.2",
    "vue-i18n": "^7.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "crx": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint-plugin-html": "^3.2.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5"
  },
  "keywords": [
    "userjs",
    "userscripts",
    "tampermonkey",
    "greasyfork",
    "vue"
